{
    "name": "mediagone/types-common-symfony",
    "type": "library",
    "description": "Provides Symfony param converters for mediagone/types-common package.",
    "keywords": ["doctrine", "value objects", "ddd", "type", "param converter"],
    "license": "MIT",
    "authors": [
        {"name": "Bruce Suire"}
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mediagone\\Symfony\\Types\\Common\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Mediagone\\Symfony\\Types\\Common\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "mediagone/symfony-powerpack": "^0.2.1|^0.3",
        "mediagone/types-common": "^0.6.5|^0.7",
        "sensio/framework-extra-bundle": "^4.0|^5.0|^6.0",
        "symfony/validator": "^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "tests": "phpunit -c phpunit.xml.dist --testsuite Unit_Tests"
    }
  }
